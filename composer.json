{
    "name": "thecodingmachine/factory-to-definition",
    "type": "library",
    "description": "This package tries to convert (if possible) factories of service providers into definition-interop compatible definitions.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TheCodingMachine\\ServiceProvider\\Converter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheCodingMachine\\ServiceProvider\\Converter\\": "tests/"
        }
    },
    "require": {
        "container-interop/definition-interop": "~0.2.0",
        "container-interop/service-provider": "~0.2.0",
        "container-interop/container-interop": "^1.0",
        "mnapoli/assembly": "~0.2.0",
        "jeremeamia/superclosure": "^2.0",
        "mouf/picotainer": "^1.0",
        "roave/better-reflection": "^1.0",
        "composer/composer": "^1.0@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5",
        "satooshi/php-coveralls": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
